                                        </div>

                                    </div>
                                    <div id="code3">
                                        <pre class="brush: js">
                                    function fact(num) {
                                    if (num < 0)
                                    return "Undefined";
                                    var fact = 1;
                                    for (var i = num; i > 1; i--)
                                    fact *= i;
                                    $("#factor").text("The factorial of " + num + " is " + fact + ".");
                                    return fact;
                                    }

                                    $("#btnmath3").click(function () {
                                    fact($("#input7").val());
                                    })
                                       </pre></div>
