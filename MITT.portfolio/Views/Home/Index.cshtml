                                    </div>
                                    <div id="code4">
                                        <pre class="brush: js">
                                    $("#btnfb").click(function () {
                                    var num1 = Number($("#fbnum1").val());
                                    var num2 = Number($("#fbnum2").val());
                                    var numbers = [];

                                    for (var loop = 1; loop <= 100; loop++) {
                                    if (loop % num1 == 0 && loop % num2 == 0) {
                                    $("#fbout").text("FizzBuzz");
                                    numbers.push("FizzBuzz");


                                    }
                                    else if (loop % num1 == 0) {
                                    $("#fbout").text("Fizz");
                                    numbers.push("Fizz");

                                    }
                                    else if (loop % num2 == 0) {
                                    $("#fbout").text("Buzz");
                                    numbers.push("Buzz");

                                    }
                                    else {
                                    $("#fbout").text("loop");
                                    numbers.push(loop);

                                    }
                                    }
                                    var output = numbers.join(', ');
                                    $("#fbout").text(output);
                                    }) </pre> </div>
